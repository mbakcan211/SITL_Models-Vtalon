    <link name="main_wheel_link">
      <pose degrees="true">0 0 0 0 0 0</pose>
      <inertial>
        <mass>0.05</mass>
        <inertia>
          <ixx>0.00044</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00078</iyy>
          <iyz>0</iyz>
          <izz>0.00044</izz>
        </inertia>
      </inertial>
